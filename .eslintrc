{
   "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
   ],
   "rules": {
      "max-len": ["off"],
      "indent": ["error", 4],
      "react/jsx-indent": ["error", 4],
      "react/jsx-indent-props": ["error", 4],
      "react/jsx-filename-extension": [
         1,
         {
            "extensions": [
               ".js"
            ]
         }
      ]
   }

}